{
  "version": "1.0",
  "providers": [
    {"name": "openai", "weight": 100}
  ],
  "circuit_breaker": {
    "enabled": true,
    "failure_threshold": 10,
    "error_rate_threshold": 0.5,
    "error_rate_window_seconds": 60,
    "timeout_ms": 30000,
    "success_threshold": 2,
    "half_open_max_calls": 3
  },
  "fallbacks": [
    {
      "when": {"status": ["circuit_breaker_open"]},
      "to": "anthropic"
    }
  ]
}

